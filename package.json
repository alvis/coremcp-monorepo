{
  "name": "coremcp-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo of CoreMCP packages",
  "keywords": [
    "mcp"
  ],
  "homepage": "https://github.com/alvis/coremcp-monorepo#readme",
  "bugs": {
    "url": "https://github.com/alvis/coremcp-monorepo/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alvis/coremcp-monorepo.git"
  },
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "pnpm --recursive run build",
    "coverage": "run coverage",
    "install": "presetter bootstrap --projects . --projects */*/",
    "lint": "run lint --",
    "prepare": "husky || true",
    "test": "run test --",
    "typecheck": "pnpm --recursive run typecheck",
    "watch": "run watch"
  },
  "lint-staged": {
    "*": "true"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "@vitest/coverage-v8": "catalog:test",
    "eslint": "catalog:lint",
    "presetter": "catalog:project",
    "presetter-preset-monorepo": "catalog:project",
    "typescript": "^5.9.0",
    "vitest": "catalog:test"
  },
  "packageManager": "pnpm@10.26.0"
}
